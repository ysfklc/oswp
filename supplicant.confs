----------------------------------------------------------------------------------
supplicant.conf file for WPA2 MGT
network={
    ssid="YOUR_SSID"
    proto=WPA2
    key_mgmt=WPA-EAP
    eap=TTLS
    eap_fast_timeout=10
    eap_identity="YOUR_IDENTITY"
    eap_anonymous_identity="anonymous@example.com"
    eap_phase2_auth=PAP
    eap_phase2_id="user=YOUR_USERNAME, password=YOUR_PASSWORD"
}
OR 
network={
    ssid="YOUR_SSID"
    proto=WPA2
    key_mgmt=WPA-EAP
    eap=PEAP
    eap_fast_timeout=10
    eap_identity="YOUR_IDENTITY"
    eap_anonymous_identity="anonymous@example.com"
    eap_phase2_auth=MSCHAPV2
    eap_phase2_id="user=YOUR_USERNAME, domain=YOUR_DOMAIN"
}
OR
network={
    ssid="YOUR_SSID"
	scan_ssid=1
	key_mgmt=WPA-EAP
	identity="domain\user"
	password="YOUR_PASSWORD"
	eap=PEAP
	phase1="peaplabel=0"
	phase2="auth=MSCHAPV2"
}
